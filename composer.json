{
    "name": "mrdth/azure-service-bus-queue",
    "description": "",
    "keywords": [
        "mrdth",
        "azure-service-bus-queue"
    ],
    "homepage": "https://github.com/mrdth/azure-service-bus-queue",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "mrdth",
            "email": "mrdth@zeventien.org",
            "role": "Developer"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": "^7.1",
        "illuminate/support": "^7.0",
        "illuminate/queue": "^7.0",
        "microsoft/windowsazure": "^0.5"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Mrdth\\AzureServiceBusQueue\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mrdth\\AzureServiceBusQueue\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mrdth\\AzureServiceBusQueue\\ServiceProvider"
            ]
        }
    }
}
